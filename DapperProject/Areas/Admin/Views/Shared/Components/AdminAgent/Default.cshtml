@model List<DapperProject.Dtos.AgentDtos.ResultAgentDto>

<div class="col-md-6 graph" style="padding-bottom: 60px;">
    <!-- Alttan padding değeri ayarlandı -->
    <div class="col-md-12">
        <div class="dash_blog">
            <div class="dash_blog_inner">
                <div class="dash_head">
                    <h3><span><i class="fa fa-comments-o"></i> Personeller</span><span class="plus_green_bt"><a href="#">+</a></span></h3>
                </div>

                <div class="msg_list_main">
                    <ul class="msg_list">
                        @foreach (var item in Model)
                        {
                            <li>
                                <span><img src="@item.ImageUrl" class="img-responsive" alt="#" /></span>
                                <span>
                                    <span class="name_user">@item.AgentName</span>
                                    <span class="msg_user">@item.Description</span>
                                </span>
                            </li>
                        }
                    </ul>
                </div>
                <div class="read_more">
                    <div class="center"><a class="main_bt read_bt" href="#">Devamı...</a></div>
                </div>
            </div>
        </div>
    </div>
</div>

